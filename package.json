{
  "name": "feedbackfruits-knowledge",
  "version": "1.4.1",
  "description": "FeedbackFruits Knowledge server",
  "main": "dist/index.js",
  "scripts": {
    "dist": "tsc",
    "clean": "rm -rf dist",
    "lint": "tslint -c tslint.json -p tsconfig.json --type-check -t codeFrame 'src/**/*.ts'",
    "lint-fix": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "prestart": "npm run dist",
    "start": "node dist",
    "pretest": "npm run clean && tsc --inlineSourceMap",
    "test": "nyc ava",
    "watch-test": "nodemon -w test -x 'npm run test'",
    "coverage": "codecov",
    "watch": "tsc --watch & nodemon; wait"
  },
  "nyc": {},
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "src/**/*.ts"
    ],
    "match": [],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": false,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "author": "Steffan Sluis <steffansluis@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.0",
    "@types/express": "^4.0.36",
    "@types/express-graphql": "0.0.33",
    "@types/flat": "0.0.28",
    "@types/graphql": "^0.8.6",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.38",
    "@types/node-fetch": "^1.6.7",
    "@types/qs": "^6.5.0",
    "@types/redis": "^0.12.36",
    "babel-runtime": "^6.23.0",
    "cayley": "^0.2.1",
    "cayley-node": "^0.1.0",
    "cors": "^2.8.4",
    "dataloader": "^1.3.0",
    "dbpedia-spotlight": "^0.5.27",
    "dotenv": "^4.0.0",
    "elasticsearch": "^12.1.3",
    "express": "^4.15.3",
    "express-graphql": "^0.6.6",
    "feedbackfruits-knowledge-context": "github:feedbackfruits/feedbackfruits-knowledge-context#feature/ontology",
    "flat": "^2.0.1",
    "graphql": "^0.10.5",
    "graphql-relay": "^0.5.2",
    "jsonld": "^0.4.12",
    "memux": "^0.2.0",
    "morgan": "^1.8.2",
    "node-cayley": "^0.1.13",
    "node-fetch": "^1.7.1",
    "p-queue": "^0.2.0",
    "qs": "^6.5.0",
    "redis": "^2.6.5",
    "semantic-graphql": "^0.6.2",
    "semantic-toolkit": "^0.2.1",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "blue-tape": "^0.2.0",
    "codecov": "^1.0.1",
    "nodemon": "^1.12.1",
    "nyc": "^11.1.0",
    "supertest": "^3.0.0",
    "tap": "^5.7.0"
  },
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  },
  "repository": "git@github.com:feedbackfruits/feedbackfruits-knowledge.git"
}
