{
  "name": "feedbackfruits-knowledge",
  "version": "1.3.0",
  "description": "FeedbackFruits Knowledge server",
  "main": "dist/index.js",
  "scripts": {
    "dist": "tsc",
    "clean": "rm -rf dist",
    "prestart": "npm run dist",
    "start": "node dist",
    "pretest": "npm run clean && tsc --inlineSourceMap",
    "test": "nyc ava",
    "watch-test": "nodemon -w test -x 'npm run test'",
    "coverage": "codecov",
    "watch": "nodemon dist"
  },
  "nyc": {},
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "src/**/*.ts"
    ],
    "match": [],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": false,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.0",
    "@types/express": "^4.0.35",
    "@types/express-graphql": "0.0.33",
    "@types/flat": "0.0.28",
    "@types/graphql": "^0.8.6",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.5",
    "@types/node-fetch": "^1.6.7",
    "@types/qs": "^6.2.30",
    "@types/redis": "^0.12.36",
    "babel-runtime": "^6.23.0",
    "cayley": "^0.2.1",
    "cayley-node": "^0.1.0",
    "cors": "^2.8.1",
    "dbpedia-spotlight": "^0.5.27",
    "dotenv": "^4.0.0",
    "elasticsearch": "^12.1.3",
    "express": "^4.14.1",
    "express-graphql": "^0.6.3",
    "flat": "^2.0.1",
    "graphql": "^0.9.1",
    "jsonld": "^0.4.11",
    "memux": "^0.2.0",
    "morgan": "^1.8.0",
    "node-cayley": "^0.1.13",
    "node-fetch": "^1.6.3",
    "p-queue": "^0.2.0",
    "qs": "^6.3.1",
    "redis": "^2.6.5",
    "typescript": "^2.1.6"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "blue-tape": "^0.2.0",
    "codecov": "^1.0.1",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.3",
    "supertest": "^3.0.0",
    "tap": "^5.7.0"
  },
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  }
}
