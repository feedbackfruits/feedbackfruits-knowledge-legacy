{
  "name": "feedbackfruits-knowledge",
  "version": "1.6.0",
  "description": "FeedbackFruits Knowledge server",
  "main": "dist/index.js",
  "scripts": {
    "dist": "tsc",
    "clean": "rm -rf dist",
    "lint": "tslint -c tslint.json -p tsconfig.json --type-check -t codeFrame 'src/**/*.ts'",
    "lint-fix": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "prestart": "npm run dist",
    "start": "node dist",
    "pretest": "npm run clean && tsc --inlineSourceMap",
    "test": "nyc ava",
    "watch-test": "nodemon -w test -x 'npm run test'",
    "coverage": "codecov",
    "watch": "tsc --watch & nodemon; wait"
  },
  "author": "Steffan Sluis <steffansluis@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@types/graphql": "14.2.2",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.7.2",
    "@types/node-fetch": "^2.5.0",
    "@types/qs": "^6.5.3",
    "@types/redis": "^2.8.13",
    "apollo-engine": "^1.1.1",
    "apollo-server": "2.1.0-alpha.10",
    "apollo-server-express": "2.1.0-alpha.10",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dataloader": "^1.3.0",
    "dotenv": "^8.1.0",
    "elasticsearch": "^16.3.0",
    "express": "^4.17.1",
    "feedbackfruits-knowledge-engine": "github:feedbackfruits/feedbackfruits-knowledge-engine#v1.4.6",
    "graphql": "14.4.2",
    "jsonld": "^1.6.2",
    "md5": "^2.2.1",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.0",
    "p-queue": "^6.1.1",
    "qs": "^6.8.0",
    "rdf-tools": "^0.5.1",
    "redis": "^2.6.5",
    "semantic-graphql": "github:knowledge-express/semantic-graphql#^0.6.4",
    "semantic-toolkit": "^0.2.1",
    "tslint": "^5.19.0",
    "typescript": "^3.6.2"
  },
  "devDependencies": {
    "ava": "^2.3.0",
    "codecov": "^3.5.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "supertest": "^4.0.2"
  },
  "repository": "git@github.com:feedbackfruits/feedbackfruits-knowledge.git"
