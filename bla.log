        { "@value" : "\u0413\u0435\u043D\u0435\u0442\u0438\u0447\u0435\u0441\u043A\u043E\u0435 \u0440\u0430\u0441\u0441\u0442\u043E\u044F\u043D\u0438\u0435" , "@language" : "ru" },
