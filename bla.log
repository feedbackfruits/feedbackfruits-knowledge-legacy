        { "@value" : "\u0416\u0438\u0437\u043D\u0435\u043D\u043D\u044B\u0439 \u0446\u0438\u043A\u043B (\u0431\u0438\u043E\u043B\u043E\u0433\u0438\u044F)" , "@language" : "ru" },
