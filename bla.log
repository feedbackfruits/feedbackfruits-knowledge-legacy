        { "@value" : "\u0424\u0438\u043B\u043E\u0433\u0435\u043D\u0435\u0442\u0438\u0447\u0435\u0441\u043A\u043E\u0435 \u0434\u0435\u0440\u0435\u0432\u043E" , "@language" : "ru" },
