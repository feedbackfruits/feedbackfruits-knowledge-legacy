        { "@value" : "\u0644\u0627\u0641\u0642\u0627\u0631\u064A\u0627\u062A" , "@language" : "ar" },
        { "@value" : "\u0411\u0435\u0441\u043F\u043E\u0437\u0432\u043E\u043D\u043E\u0447\u043D\u044B\u0435" , "@language" : "ru" },
